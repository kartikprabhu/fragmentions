{
	"manifest_version": 2,
	"name": "Fragmentions",
	"short_name": "Fragmentions",
	"description": "Use ## double-hash anchors as links to individual words or phrases on in a document.",
	"version": "0.6.1",
	"permissions": ["contextMenus", "tabs"],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"all_frames": true,
		"js": ["content.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	}],
	"icons": {
		"32": "icon-32.png",
		"128": "icon.png"
	}
}
